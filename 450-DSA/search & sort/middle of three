//https://practice.geeksforgeeks.org/problems/middle-of-three2926/1
class Solution{
  public:
    int middle(int A, int B, int C){
        //code here//Position this line where user code will be pasted.
        int sum=A+B+C;
        int ans=sum-(max(max(A,B),C))-(min(min(A,B),C));
        return ans;
    }
}
