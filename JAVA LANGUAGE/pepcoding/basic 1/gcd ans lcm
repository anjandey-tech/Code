import java.util.*;
    
    public class Main{
    
    public static void main(String[] args) 
    {
        Scanner scn = new Scanner(System.in);
        
        int n=scn.nextInt();
        int m=scn.nextInt();
        int ans=gcd(n,m);
        int ans1=(n*m)/ans;
        System.out.println(ans);
         System.out.println(ans1);
        
       
     }
    
   public static int gcd(int a,int b)
{
	if(b==0)
	return a;
  return gcd(b,a%b);
 
 }
    }
