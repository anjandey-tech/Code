#include <bits/stdc++.h>
#define ll long long 
using namespace std;

int main() {
	// your code goes here
	ll t;
	cin>>t;
	while(t--)
	{
	  ll int n;
	   cin>>n;
	   ll int arr[n];
	   for(ll int i=0;i<n;i++)
	   {
	      cin>>arr[i];
	   }
	   bool decreasing = true;
	   for(ll int i=0;i<n-1;i++)
	   {
	      if(arr[i]<arr[i+1])
	      decreasing=false;
	   }
	   if(decreasing)
	   cout<<"NO\n";
	   else
	   cout<<"YES\n";
	}
	return 0;
}
