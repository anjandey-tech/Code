https://www.codechef.com/problems/SUMPOS
#include <bits/stdc++.h>
using namespace std;

int main() {
	 int t;
	 cin>>t;
	 
	 while(t--)
	 {
	    int x,y,z;
	    cin>>x>>y>>z;
	     int arr[3];
	     arr[0]=x;
	     arr[1]=y;
	     arr[2]=z;
	     sort(arr,arr+3);
	     if((arr[0]+arr[1])==arr[2])
	     {
	        cout<<"YES"<<endl;
	     }
	     else{
	        cout<<"NO"<<endl;
	     }
	 }
	return 0;
}
